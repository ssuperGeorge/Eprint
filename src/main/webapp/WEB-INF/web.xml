<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

		<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.eprint.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>com.eprint.servlet.SignupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Eprint</servlet-name>
		<servlet-class>com.eprint.servlet.EprintServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Eprint</servlet-name>
		<url-pattern>/eprint</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>AppID</param-name>
		<param-value>U48s1uRRkv00K0pftMusCM6w-MdYXbMMI</param-value>
	</context-param>
	<context-param>
		<param-name>AppKey</param-name>
		<param-value>Ru33PcVKprXuiO82Oxep8bov</param-value>
	</context-param>

	<welcome-file-list>
		<welcome-file>login</welcome-file>
		<welcome-file>./homepage.html</welcome-file>
	</welcome-file-list>
</web-app>